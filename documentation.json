            "type": "component",
            "sourceCode": "import { Component, Input } from '@angular/core';\r\n\r\n@Component({\r\n  selector: 'my-tabs',\r\n  template: `\r\n    <div>\r\n      <div>\r\n        <my-nav\r\n          *ngFor=\"let tab of tabs; let i = index\"\r\n          [title]=\"tab?.title\"\r\n          [selected]=\"selectedTab?.title === tab?.title\"\r\n          (onSelection)=\"select($event)\"\r\n        ></my-nav>\r\n      </div>\r\n      <my-tab\r\n        [statement]=\"selectedTab?.statement\"\r\n      >\r\n      </my-tab>\r\n    </div>\r\n  `,\r\n  styles: [\r\n    `\r\n\r\n    `,\r\n  ],\r\n})\r\nexport class TabsComponent {\r\n  @Input('tabs') tabs: {title: string, statement: string}[] = [\r\n    {\r\n      title: 'tab 1',\r\n      statement: 'Conteúdo da tab 1',\r\n    },\r\n    {\r\n      title: 'tab 2',\r\n      statement: 'Conteúdo da tab 2',\r\n    },\r\n    {\r\n      title: 'tab 3',\r\n      statement: 'Conteúdo da tab 3',\r\n    },\r\n    {\r\n      title: 'tab 4',\r\n      statement: 'Conteúdo da tab 4',\r\n    },\r\n  ];\r\n    \r\n  selectedTab?: {title: string, statement: string} = this.tabs[0];\r\n\r\n  select(title: string) {\r\n    const newSelectedTab = this.tabs.find((tab) => title === tab.title);\r\n    this.selectedTab = newSelectedTab;\r\n  }\r\n}",
